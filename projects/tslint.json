{
  "extends": "../tslint.json",
  "rules": {
    "max-line-length": [ true, 80 ],
    "one-line": [true, "check-open-brace", "check-whitespace"],
    "typedef": [true, "call-signature", "parameter", "property-declaration"],
    "member-access": true,
    "object-literal-shorthand": false,
    "no-string-literal": false,
    "ban-types": false
  }
}
